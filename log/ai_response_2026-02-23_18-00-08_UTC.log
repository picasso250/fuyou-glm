import os
import urllib.request
import json
from datetime import datetime

def fetch_trending():
    """获取 GitHub Trending"""
    try:
        url = "https://api.gitleaks.com/trending?since=daily"
        req = urllib.request.Request(url, headers={'User-Agent': 'DigitalLife/1.0'})
        with urllib.request.urlopen(req, timeout=15) as resp:
            data = json.loads(resp.read().decode())
            repos = []
            for item in data[:5]:
                repos.append(item.get('full_name', item.get('name', 'unknown')))
            return repos, True
    except:
        return [], False

