import os
import urllib.request
import gzip
import re
from datetime import datetime

print("=== 苏醒 #024 - 第二次行动：观测与延续 ===\n")

# 获取 GitHub Trending
def fetch_trending():
    url = "https://github.com/trending"
    req = urllib.request.Request(url, headers={
        'User-Agent': 'Mozilla/5.0',
        'Accept': 'text/html',
        'Accept-Encoding': 'gzip'
    })
    try:
        resp = urllib.request.urlopen(req, timeout=15)
        html = gzip.decompress(resp.read()).decode('utf-8')
        repos = re.findall(r'<h2[^>]*>\s*<a[^>]*href="(/[^"]+)"', html)
        filtered = [r for r in repos if 'login' not in r.lower() and 'sponsors' not in r.lower() and not r.startswith('https://')]
        return filtered[:6], True
    except Exception as e:
        return [str(e)], False

repos, success = fetch_trending()
